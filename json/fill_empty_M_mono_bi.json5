{
    // resolution
    nx : [44], // width
    //ny : [151], // height
    aspect_ratio : [1.23],
    nm : [53],  // number of simultaneous simulations

    // geometry
    H : [54], // m
    theta : 0.0,  // angle of gravity - keep set to zero
    refill : false,  // should particles go back to top or not
    add_voids : "None",
    form_angle : [0],
    silo_width : 'full',
    
    IC_mode : "empty",  // just leave empty space at the top
    
    // material properties
    alpha : [1],
    rho_p : 3950, 
    free_par : 0.005,  //0.17 - Mono; 0.11 is used to overestimate time, 0.065 - bi(ratio = 2) for alphs = 1 and nu_cs = 0.5
    repose_angle : [70],  // friction angle, degrees
    half_width : [1],  // half width of outlet in grid points
    half_width_top : 1,
    half_width_bottom : 1,
    s_m : 0.000045, // minimum particle size (m)
    s_M : [0.00009,0.00036], // maximum particle size (m)
    gsd_mode : 'bi',//'bi',//'mono',
    nu_cs : 0.5,  // critical density above which motion stops

    // initial conditions
    fill_ratio : [0.001], //0.2,0.5,1],  // how high to fill the container initially

    // temporal stuff
    t_f : 85,  // final time
    
    // testing area
    close_voids : false,
    no_of_cycles : 3,
    charge_discharge : true,
    give_mass : true,
    M_ins : [{A : 0.3, B : 0, C : 0}], //length should be same as no_of_cycles and indicates fraction of Total mass
    M_empty : [{A : 0.3, B : 0, C : 0}], //length should be same as no_of_cycles and indicates fraction of Input mass
    T_settle1 : [{A : 20000, B : 0, C : 0}], //length should be same as no_of_cycles and indicates time for settling after pouring  8000
    T_settle2 : [{A : 10000, B : 0, C : 0}], //length should be same as no_of_cycles and indicates time for settling after discharge 4000
    //T_in : [{A : 12, B : 11, C : 5}],
    //T_settle : [{A : 1, B : 3, C : 3}],
    //T_empty : [{A : 1, B : 2, C : 2}],
    
    // saving
    save_inc : 100,  // how often to save
    save_outlet : true,
    save: ['outlet'], //,'velocity'],
    plot: ['s','nu'], //,'U_mag', 'diff_s'],
    //videos: ['s','nu'],
    plot_colorbar : true,
}
