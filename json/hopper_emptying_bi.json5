{
    // resolution
    nx : [51],
    ny : [101],
    nm : [50],  // number of simultaneous simulations

    // geometry
    H : 0.1, // m
    theta : 0.0,  // angle of gravity - keep set to zero
    refill : false,  // should particles go back to top or not
    add_voids : "central_outlet",
    
    // material properties
    // mu : [0.5,1.0,2.0],  // friction angle
    alpha : [1],
    mu : [1.0], //,0.1,10.0],  // friction angle
    half_width : [12],  // half width of outlet in grid points
    s_m : 0.000045, // minimum particle size (m)
    s_M : 0.00009, // maximum particle size (m)
    gsd_mode : 'bi',
    nu_cs : 0.5,

    // initial conditions
    // IC_mode : "top",  // just leave empty space at the top
    //fill_ratio : 0.7,  // how high to fill the container initially
    IC_mode : 'random',
    nu_fill : [0.7],
    //charge_discharge : false,
    //T_cycles : [],

    // temporal stuff
    t_f : 60.0,  // final time
    
    // saving
    save_inc : 5,  // how often to save
    save_outlet : true,
    save: ['outlet'],
    plot: ['s','nu'],
    videos: ['s','nu'],

    // testing area
    close_voids : false,
    plot_colorbar : true,
}
